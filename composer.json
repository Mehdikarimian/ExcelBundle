{
    "name": "Mehdikarimian/excelbundle",
    "description": "This is a Symfony4 Bundle helps you to read and write Excel files (including pdf, xlsx, odt), thanks to the PHPExcel library",
    "type": "symfony-bundle",
    
    "authors": [
        {
            "name": "Mehdi karimian",
            "email": "Mehdikarimian94@gmail.com"
        },
        {
            "name": "ExcelBundle Contributors",
            "homepage": "https://github.com/liuggio/ExcelBundle#contributors"
        }
    ],

    "keywords": ["xls","Excel", "symfony2", "bundle"],
    "homepage": "http://www.welcometothebundle.com",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Mehdikarimian/ExcelBundle"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "~2.6|~4.0",
        "phpoffice/phpexcel": "~1.8.1",
        "Mehdikarimian/excelbundle": "dev-bugfix"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "symfony/finder": "~2.6|~3.0",
        "symfony/form": "~2.6|~3.0",
        "symfony/class-loader": "~2.6|~3.0",
        "symfony/validator": "~2.6|~3.0",
        "symfony/browser-kit": "~2.6|~3.0",
        "sensio/framework-extra-bundle": "~2.3|~3.0"
    },
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "Liuggio\\ExcelBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liuggio\\ExcelBundle\\": ""
        }
    },
    
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }, 
    "config": {
        "bin-dir": "bin"
    },
    "version": "4.0.0"
}
